@model IEnumerable<YOUTUBE.Models.innerjoin>
@{
    ViewBag.Title = "showlastvideo";
    Layout = "~/Views/Shared/_LayoutMain.cshtml";
}
@foreach (var item in Model)
{
    <div class="right-sidebar">

        <div class="side-video-list">
            <a href="" class="small-thumbnail"><img src="@item.videoo.imagetitle"></a>
            <div class="vid-info">
                <a href=""> @item.videoo.nametitle</a>
                <p>@item.accountt.username</p>
                <p>15k lượt xem</p>
            </div>
        </div>

    </div>

}

